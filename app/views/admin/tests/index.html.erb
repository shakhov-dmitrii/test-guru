<div class="container">  
  <div class="row">
    <div class="col">    
      <h1><%= t('.header') %></h1>
      <p><%= link_to t('.create_test'), new_admin_test_path %> | <%= link_to t('.gists'), admin_gists_path %></p>
      <table class="table">
        <tr>
          <th class="sort-by-title pointer">
            <%= t('.title') %>
            <%= octicon 'arrow-up', class: 'text-success hide' %>
            <%= octicon 'arrow-down', class: 'text-success hide' %>
          </th>
          <th><%= t('.category') %></th>
          <th><%= t('.level') %></th>
          <th><%= t('.questions') %></th>
          <th colspan="2"><%= t('.action') %></th>
        </tr>
        
        <%= render @tests %>

      </table>
    </div>
  </div>
</div>
